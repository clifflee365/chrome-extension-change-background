{
  "manifest_version": 3,
  "name": "ChangeBG",
  "description": "Change page background color easily",
  "icons": {
    "32": "icon32.png"
  },
  "version": "0.0.1",
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "action": {
      "default_popup": "index.html"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}